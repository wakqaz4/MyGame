<ui version="1.0">

<!--
/* PanelViewCube
 */
-->
<hbox name="main_widget" export="1" space="0" height="120" width="120" background="0">
	<hbox name="ortho_hbox" export="1" background="0" align="expand">
		<icon name="ortho_i" export="1" texture="editor/gui/action_ortho.png" toggleable="1" toggled="1" align="right,top" width="24" height="24">
			<callback type="clicked" widget="ortho_i">ortho_clicked</callback>
			<tooltip>Switch between the orthographic and perspective projections</tooltip>
		</icon>
	</hbox>
</hbox>
</ui>
